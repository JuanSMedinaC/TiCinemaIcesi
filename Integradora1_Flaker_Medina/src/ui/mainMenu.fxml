<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainMenuController">
   <children>
      <VBox alignment="CENTER" minHeight="300.0" minWidth="800.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox alignment="CENTER" prefHeight="39.0" prefWidth="800.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#showRegisterMovies" text="Register movie" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="42.0" prefWidth="800.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#showMoviesCatalog" text="Show movies" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="39.0" prefWidth="800.0">
               <children>
                  <Button mnemonicParsing="false" text="Button" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</VBox>
